            <button class="btn btn-primary btn-md px-4 me-md-2" onclick="planForm();">Create Plan</button>

      </div>       
      <div>
       <form action="/logout" method="GET">  
      <input class="btn btn-primary btn-md px-4 me-md-2 logout" type="submit" value="Logout">
    </form>
    </div>
  </div>
      <div id="planContainer" class="row">
    </div>
